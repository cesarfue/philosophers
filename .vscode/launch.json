{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["6", "410", "200", "200"],
      "stopAtEntry": false,
      "externalConsole": false,
    //   "cwd": "/home/cesar/Cursus/philosophers",
    //   "program": "/home/cesar/Cursus/philosophers/philo",
		"cwd": "${workspaceFolder}",
		"program": "${workspaceFolder}/philo",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}